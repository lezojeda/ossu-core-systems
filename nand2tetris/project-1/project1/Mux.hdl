/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);
    And(a=notSel, b=a, out=and2);
    And(a=sel, b=b, out=and1);
    Or(a=and1, b=and2, out=out);
    //// Replace this comment with your code.
}