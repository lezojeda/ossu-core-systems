class Point {
    field int x, y;
    static int pointCount;

    constructor Point new(int ax, int ay) {
        let x = ax;
        let y = ay;
        let pointCount = pointCount + 1;
        return this;
    }

    method int getX(int a, int b, int c) {
        return x;
    }

    method int getY() {
        return y;
    }
}
